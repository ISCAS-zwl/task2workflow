{
  "mcpServers": {
    "bingcnmcp": {
        "command": "cmd",
        "args": [
          "/c",
          "npx",
          "bing-cn-mcp"
      ]
    },
     "12306-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "12306-mcp"
            ]
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "trends-hub": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-trends-hub@1.6.0"
      ]
    },
    "mcp-deepwiki": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-deepwiki@latest"
      ]
    },
    "arxiv-mcp-server": {
        "command": "uv",
        "args": [
            "tool",
            "run",
            "arxiv-mcp-server",
            "--storage-path", "D:/mcpfile"
        ]
    },
     "excel": {
        "command": "npx",
        "args": ["--yes", "@negokaz/excel-mcp-server"],
        "env": {
            "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
        }
    },
    "filesystem": {
        "args": [
            "-y",
            "@modelcontextprotocol/server-filesystem",
            "D:/mcpfile"
        ],
        "command": "npx"
        
    },
    "mcp-server-chart": {
        "command": "npx",
        "args": [
            "-y",
            "@antv/mcp-server-chart"
        ]
    },
    "mcp-server-time": {
    "command": "uvx",
    "args": ["mcp-server-time"]
  }

  }
}